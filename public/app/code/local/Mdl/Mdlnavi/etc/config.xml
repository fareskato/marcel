<?xml version="1.0"?>
<config>
	<modules>
		<Mdl_Mdlnavi>
			<version>1.1.0</version>
		</Mdl_Mdlnavi>
	</modules>
	<frontend>
		<layout>
			<updates>
				<mdlmdlnavi>
					<file>mdl/mdlnavi.xml</file>
				</mdlmdlnavi>
			</updates>
		</layout>
		<translate>
			<modules>
				<Mdl_Mdlnavi>
					<files>
						<default>Mdl_Mdlnavi.csv</default>
					</files>
				</Mdl_Mdlnavi>
			</modules>
		</translate>
	</frontend>
	<global>
		<blocks>
			<mdlmdlnavi>
				<class>Mdl_Mdlnavi_Block</class>
			</mdlmdlnavi>
		</blocks>
		<helpers>
			<mdlmdlnavi>
				<class>Mdl_Mdlnavi_Helper</class>
			</mdlmdlnavi>
		</helpers>
		<models>
			<mdlmdlnavi>
				<class>Mdl_Mdlnavi_Model</class>
			</mdlmdlnavi>
		</models>
		<resources>
			<mdlmdlnavi_setup>
				<setup>
					<module>Mdl_Mdlnavi</module>
					<class>Mage_Catalog_Model_Resource_Eav_Mysql4_Setup</class>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</mdlmdlnavi_setup>
			<mdlmdlnavi_write>
				<connection>
					<use>core_write</use>
				</connection>
			</mdlmdlnavi_write>
			<mdlmdlnavi_read>
				<connection>
					<use>core_read</use>
				</connection>
			</mdlmdlnavi_read>
		</resources>
	</global>
	<default>
		<mdlmdlnavi>
			<general>
				<status>1</status>
			</general>
			<navsettings>
				<navoption_home>1</navoption_home>
				<nav-fix>1</nav-fix>
				<navleftposhome>1</navleftposhome>
				<navleftposhomecat>1</navleftposhomecat>
			</navsettings>
		</mdlmdlnavi>
	</default>
</config>